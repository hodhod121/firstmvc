<div class="m-auto w-50 bg-danger text-center">
    
    @{
        string Bred = "35px";
        for (int i = 1; i < 11; i++)
        {
            for (int j = 1; j < 11; j++)
            {
                <p style="width:@Bred;display:inline-block;">@(i*j)</p>
            }
            <br />
        }
    }
</div>